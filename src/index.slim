	body
		hr.tall
		main.main role="main"
			header.card-wrapper.start role="banner"
				h1.page-title
					a href="https://bennettfeely.com/antiweather/" Antiweather
				h2 How's the weather on the opposite side of the world?
				noscript
					p.no-script-message This site uses javascript to retrieve weather and location information. Try running in an browser with javascript enabled.
				.columns
					.search-wrapper role="search"
						.form
							.location-finder
								p Enter a location to find out:
								.search-input-wrapper#autocomplete.autocomplete
									input#search.search-input.autocomplete-input type="text" placeholder="Your location..." spellcheck="false" autocomplete="off"
									.search-button
										svg.search-icon
											use xlink:href="#search-icon"
									ul.autocomplete-result-list
						p Or try these opposite points on Earth:
						nav.pre-location-wrapper role="navigation"
							button.pre-location type="button" onclick="getWeather('home', 90, 0, 'North Pole');getWeather('away', -90, 0, 'South Pole');" 
								span North Pole
								span.dash ↔
								span South Pole
							button.pre-location type="button" onclick="getWeather('home', -36.85, 174.76, 'Auckland, New Zealand');getWeather('away', 37.38, -5.99, 'Ronda, Spain');" 
								span Auckland
								span.dash ↔
								span Ronda
							button.pre-location type="button" onclick="getWeather('home', 25.04, 121.57, 'Taipei, Taiwan');getWeather('away', -25.04, -58.43, 'Asuncion, Paraguay');" 
								span Taipei
								span.dash ↔
								span Asuncion
							button.pre-location type="button" onclick="getWeather('home', 47.92, 106.92, 'Ulaanbaatar, Mongolia');getWeather('away', -47.92, -73.08, 'Tortel, Chile');" 
								span Ulaanbaatar
								span.dash ↔
								span Tortel
							button.pre-location type="button" onclick="getWeather('home', 21.31, -157.86, 'Honolulu, Hawaii');getWeather('away', -21.31, 22.14, 'Ghanzi District, Botswana');"
								span Honolulu
								span.dash ↔
								span Ghanzi
							button.pre-location type="button" onclick="getWeather('home', 31.22, 121.49, 'Shanghai, China');getWeather('away', -31.39, -57.96, 'Salto, Uruguay');" 
								span Shanghai
								span.dash ↔
								span Salto
						.units-wrapper
							p Units:
							input id="c_toggle" type="radio" name="units-toggle" class="c-input" value="si"
							label for="c_toggle" class="unit-toggle c-toggle" &deg;C
							input id="f_toggle" type="radio" name="units-toggle" class="f-input" value="us" checked="checked"
							label for="f_toggle" class="unit-toggle f-toggle" &deg;F
					.quick-wrapper
						.quick-card-heading Quick Summary
						.quick-card-wrapper
							- for location in ["home", "away"]
								section.quick-card class="quick-#{location}"
									.quick-card-content
										.quick-location -------------
										.quick-temperature --&deg;
										.quick-summary --------
					.ad-wrapper
						script async="async" type="text/javascript" src="//cdn.carbonads.com/carbon.js?serve=CKYIL27N&placement=bennettfeelycom" id="_carbonads_js"
			.results-wrapper
				- for location in ["home", "away"]
					section.card-wrapper class="#{location}"
						.loading 
							.loading-spinner
							| Loading...
						.card
							.map-wrapper
								.map
								.marker
									svg width="30" height="30" xmlns="http://www.w3.org/2000/svg" viewBox="0 -10 14 35"
										ellipse fill="rgba(0,0,0,.25)" cx="7" cy="20" rx="5" ry="2"
										path fill="currentColor" d="M7 0C3.13 0 0 3.13 0 7c0 5.25 7 13 7 13s7-7.75 7-13c0-3.87-3.13-7-7-7zm0 9.5C5.62 9.5 4.5 8.38 4.5 7S5.62 4.5 7 4.5 9.5 5.62 9.5 7 8.38 9.5 7 9.5z"
							.currently-wrapper
								.geo-wrapper
									.value.coordinates ----------, ----------
									// .value.elevation -----
								.location-wrapper
									.value.location -------------
								.temperature-wrapper
									.temperature --&deg;
								.summary-wrapper
									.value.summary ---- ----- ----- --- -- -------
								.grid
									.grid-item.full
										.grid-label Local time:
										.grid-value.time --:-- --
									.grid-item.full
										.grid-label Local date:
										.grid-value.date ---------, ------ --, ----
									.grid-item.half
										.grid-label Feels like:
										.grid-value.apparenttemperature --&deg;
									.grid-item.half
										.grid-label Cloudiness: 
										.grid-value.cloudcover --%
									.grid-item.half
										.grid-label Wind:
										.grid-value.wind -- ----
									.grid-item.half
										.grid-label Wind Gust:
										.grid-value.windgust -- ----
									.grid-item.half
										.grid-label Humidity:
										.grid-value.humidity --%
									.grid-item.half
										.grid-label Dew Point:
										.grid-value.dewpoint --&deg;
									.grid-item.half
										.grid-label Pressure:
										.grid-value.pressure ---- --
									.grid-item.half
										.grid-label Visibility:
										.grid-value.visibility -- ---
									.grid-item.half
										.grid-label UV:
										.grid-value.uv ----
									.grid-item.half
										.grid-label Ozone:
										.grid-value.ozone ----
									.grid-item.half
										.grid-label Sunrise:
										.grid-value.sunrise --:-- --
									.grid-item.half
										.grid-label Sunset:
										.grid-value.sunset --:-- --
							.forecast-heading-wrapper
								.value.forecast-heading
							.forecast-wrapper
		hr.short
